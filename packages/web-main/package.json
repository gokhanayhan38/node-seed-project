    "web-components": "file:../web-components",
    "flux": "^2.1.1",
    "jquery": "^2.2.4",
    "less": "^2.7.1",
    "node-fetch": "1.6.0",
    "react": "^15.0.2",
    "react-context-wrapper": "0.0.5",
    "react-dom": "^15.0.2",
