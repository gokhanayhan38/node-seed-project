{
  "name": "http-serve",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "http-server": "^0.9.0"
  },
  "scripts": {
    "clean": "rm -rf dir-to-http-serve && mkdir dir-to-http-serve",
    "copy-data": "rm -rf dir-to-http-serve/data && cp -r ../../data dir-to-http-serve",
    "start": "http-server dir-to-http-serve -p 9001 --cors"
  }
}
